  "scripts": {
    "dev:watch": "concurrently \"webpack --config ./webpack/server.config.ts -w\" \"webpack --config ./webpack/client.config.ts -w\"",
    "dev:server": "nodemon --inspect build/debug/server/server.js -w ./build/debug/server",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:watch\"",
    "build:server": "webpack --config ./webpack/server.config.ts --node-env=production",
    "build:client": "webpack --config ./webpack/client.config.ts --node-env=production",
    "build-and-run": "concurrently \"yarn build:server\" \"yarn build:client\" && node build/server/server.js",
    "db:migrate": "ts-node ./migrations/migration.ts"
  }
